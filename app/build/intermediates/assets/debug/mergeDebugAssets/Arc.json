{
    "barcode": {
        //@config(0015, 0001)
        //  Barcode device type:
        //    0 = none
        //    1 = Informatics Wasp WCS3905 CCD Scanner (Not supported in ARC)
        //    2 = iSMP or iSMP Companion (Not supported in ARC)
        //    3 = Ingenico Tetra integrated Laser scanner
        //    4 = Ingenico Tetra integrated Camera scanner
        "deviceType": "0",
        //@config(0015, 0008)
        //  Enabled symbologies - A comma-separated list of (non-negative) decimal codes
        //                       corresponding to barcode symbologies to enable.
        //
        //  "":           NO barcode symbologies enabled; ALL symbologies disabled
        //  "0" or "00":  ALL barcode symbologies enabled. Must be only code in list.
        //
        //  "1"/"01": EAN-13                   "27": Telepen
        //  "2"/"02": EAN-8                    "29": Code 39 CPI (aka "Italian CPI")
        //  "3"/"03": UPC-A                    "30": Codablock A
        //  "4"/"04": UPC-E                    "31": Codablock F
        //  "7"/"07": UPC-A  with addon2       "33": PDF417
        //  "8"/"08": UPC-E  with addon2       "34": GS1-128
        //      "11": UPC-A  with addon5       "35": ISBT128
        //      "12": UPC-E  with addon5       "36": Micro PDF
        //      "13": Code39                   "37": GS1 DataBar Omni-Directional
        //      "15": Interleaved 2 of 5       "38": GS1 DataBar Limited
        //      "16":    Standard 2 of 5       "39": GS1 DataBar Expanded
        //      "17":      Matrix 2 of 5       "40": DataMatrix
        //      "19": Codabar                  "41": QR Code
        //      "21": MSI                      "42": MaxiCode
        //      "22": Plessey                  "74": AZTEC
        //      "23": Code 128
        //      "25": Code 93
        //      "26": Code 11
        //
        //  Examples:
        //      " "            to disable ALL barcode symbologies
        //      "00"           to enable  ALL barcode symbologies
        //      "13,23,33,41"  to enable Code39, Code128, PDF417, and QR barcodes
        //
        // Not supported by Move/5000.
        "enabledSymbologies": "",
        //@config(0015, 0005)
        //  Illumination mode
        //    0 = Scan and aimer LED lights off (no illumination)
        //    1 = Scan           LED lights on
        //    2 = Aimer          LED lights on
        //    3 = Scan and aimer LED lights on
        "illuminationMode": "3",
        //@config(0015, 0004)
        //  Image mode
        //    1 = 1D barcodes only
        //    2 = 1D and 2D barcodes
        //    3 = 1D and 2D barcodes for bright environments
        //    4 = 1D and 2D barcodes for shiny/reflective surfaces
        //
        //  Not supported by Move/5000.
        "imageMode": "2",
        //@config(0015, 0006)
        //  Lighting mode
        //    0 = Shorter exposure time
        //    1 = Longer  exposure time (for shiny/reflective surfaces)
        //
        //  Not supported by Move/5000.
        "lightingMode": "0",
        //@config(0015, 0003)
        //  Scan mode
        //    1 = Single scan
        //    2 = Multi  scan
        "scanMode": "1",
        //@config(0015, 0010)
        //  Scan timeout - Timeout in millis
        "scanTimeoutMs": "2000",
        //@config(0015, 0007)
        //  Trigger enabled to scan barcodes (Move/5000 only)
        //
        //  Not supported by Move/5000.
        "triggerEnabled": "false"
    },
